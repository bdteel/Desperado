          <argument>
            <kind>1</kind>
            <string>    ///Main Logic
    
    //====Tick
    vsp += grv;
    hsp = Approach(hsp,0,frc);
    if (iframes &gt; 0) {iframes--}

    if (hit != 0)
    {
        if (iframes = 0){skel_hp += -1;}
        iframes = 15
        if (skel_hp &lt;=0) 
        {
            instance_destroy();
            repeat(choose(2,3)) instance_create(x,bbox_bottom,obj_dust);
            score += 10;
            scr = "Score: "
            window_set_caption(scr + string(score));
        }
        image_index = 1;
        hit = Approach(hit,0,0.05);
    }
    else image_index = 0;
    
    //===Move
    if (instance_exists(obj_player)) &amp;&amp; (instance_exists(obj_skeleton))
    {
        if (obj_player.x &gt; x)  { image_xscale = -1; }
        if (obj_player.x &lt; x)  { image_xscale = 1; }
        face = obj_player.x - x;
    }
    if (face &gt; 1) &amp;&amp; (hit = 0) {hsp = 1;}
    if (face &lt; -1) &amp;&amp; (hit = 0) {hsp = -1;}
    if (face = 0 || face = 1 || face = -1) &amp;&amp; (hit = 0) {hsp = 0;}
    
    //===Collide
    Player_MoveCollide();
    
   //===Player Collision
    
    if (hit == 0) &amp;&amp; (obj_player.state != st.dodge) &amp;&amp; position_meeting(x, y, obj_player)
    {
        obj_player.hit = 1;
        obj_player.vsp = -3;
        obj_player.hsp = sign(face) * 8;
        obj_player.image_xscale = sign(hsp);
    }
                
                    
</string>
